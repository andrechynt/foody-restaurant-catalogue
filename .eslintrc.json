{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "import/no-extraneous-dependencies": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "no-unused-expressions": 0,
        "array-callback-return": 0,
        "object-shorthand": 0,
        "no-alert": 0,
        "no-restricted-globals": 0,
        "no-plusplus": 0
    }
}
